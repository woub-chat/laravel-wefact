{
    "name": "bfg/laravel-wefact",
    "description": "Wefact for Laravel 5.8-9.x",
    "keywords": ["wefact", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "6.*|7.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5|^8.3",
        "orchestra/testbench": "^3.8|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Wefact\\": "src/",
            "Bfg\\Wefact\\tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Wefact\\ServiceProvider"
            ],
            "aliases": {
                "Wefact": "Bfg\\Wefact\\Facade"
            }
        }
    }
}
